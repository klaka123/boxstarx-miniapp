@dp.message()
async def webapp_handler(message: types.Message):
    if not message.web_app_data: return
    data=json.loads(message.web_app_data.data)
    if data["action"]=="topup":
        amount=int(data["amount"])
        kb=InlineKeyboardMarkup(inline_keyboard=[
            [InlineKeyboardButton(text=f"ğŸ ĞŸĞ¾Ğ´Ğ°Ñ€Ğ¸Ñ‚ÑŒ {amount} â­ Ğ±Ğ¾Ñ‚Ñƒ", pay=True)]
        ])
        await message.answer(f"ĞŸĞ¾Ğ¿Ğ¾Ğ»Ğ½ĞµĞ½Ğ¸Ğµ {amount} â­ Ğ² @BoxstarxBot", reply_markup=kb)
